{
  "extends": "defaults/configurations/google",

  "env": {
    "browser": 1
  },

  "globals": {
    "require": 1,
    "$": 1,
    "hexo": 1
  },

  "rules": {
    "max-len": [1, 120, 4],
    "no-invalid-this": 0,
    "dot-location": 0,
    "brace-style": [2, "stroustrup", { "allowSingleLine": true }]
  }
}
