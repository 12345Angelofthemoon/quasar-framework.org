<aside id="qwb-device-preview">
  <div id="qwb-device-preview-chooser" class="btn-group">
    <a id="qwb-device-android-preview-btn" data-theme="android" class="btn icon-only active" title="Android">
      <i class="fa fa-android"></i>
    </a>
    <a id="qwb-device-apple-preview-btn" data-theme="apple" class="btn icon-only" title="iOS">
      <i class="fa fa-apple"></i>
    </a>
    <!--<a id="qwb-device-win-preview-btn" data-theme="win" class="btn inverted icon-only" title="Windows">
      <i class="fa fa-windows"></i>
    </a>-->
    <a id="qwb-device-desktop-preview-btn" class="btn icon-only desktop-launcher" target="_blank" title="Desktop View">
      <i class="fa fa-desktop"></i>
    </a>
    <a class="btn icon-only view-source" target="_blank" title="View Source">
      <i class="fa fa-file-code-o"></i>
    </a>
  </div>

  <div id="qwb-android-preview" class="theme active">
    <iframe id="android-iframe" frameborder="0"></iframe>
  </div>
  <div id="qwb-apple-preview" class="theme">
    <iframe id="apple-iframe" frameborder="0"></iframe>
    <img class="platform-statusbar" src="/images/ios-statusbar.png">
  </div>
  <!--<div id="qwb-win-preview" class="theme">
    <iframe id="win-iframe" frameborder="0"></iframe>
  </div>-->
</aside>
